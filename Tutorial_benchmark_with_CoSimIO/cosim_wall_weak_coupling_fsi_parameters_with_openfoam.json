{
    "problem_data" :
    {
        "start_time" : 0.0,
        "end_time" : 100.0,
        "echo_level" : 4,
        "print_colors" : true,
        "parallel_type" : "OpenMP"
    },
    "solver_settings" :
    {
        "type" : "coupled_solvers.gauss_seidel_weak",
        "echo_level" : 4,
        "predictors" : [
            {
                "type" : "average_value_based",
                "solver"         : "Openfoam_Kratos_Wrapper",
                "data_name"      : "load_cells",
                "echo_level" : 4
            }
        ],
        "data_transfer_operators" : {
            "mapper" : {
                "type" : "kratos_mapping",
                "mapper_settings" : {
                    "mapper_type" : "nearest_neighbor"
                }
            }
        },
        "coupling_sequence":
        [
        {
            "name": "structure",
            "input_data_list": [
                {
                    "data"              : "load",
                    "from_solver"       : "Openfoam_Kratos_Wrapper",
                    "from_solver_data"  : "load",
                    "data_transfer_operator" : "mapper",
                    "before_data_transfer_operations" : ["mapping_element_node_1"],
                    //"after_data_transfer_operations" : ["scaling_loads_2","scaling_loads_3","scaling_loads_4","scaling_loads_5","scaling_loads_6","scaling_loads_7"],
                    "after_data_transfer_operations" : ["scaling_loads_1"],
                    "data_transfer_operator_options" : ["swap_sign", "use_transpose"]
                    ///"after_data_transfer_operations" : ["scaling_loads_1","scaling_loads_2"],
                    //"interval" : [0.4, 2.0]
                }
            ],
            "output_data_list": [
                {
                    "data"           : "disp",
                    "to_solver"      : "Openfoam_Kratos_Wrapper",
                    "to_solver_data" : "disp",
                    "data_transfer_operator" : "mapper"
                }
            ]
        },
        {
            "name": "Openfoam_Kratos_Wrapper",
            "input_data_list": [],
            "output_data_list": []
        }
        ],
        "coupling_operations" : {
            "scaling_loads_1" : {
                "type"           : "scaling",
                "solver"         : "structure",
                "data_name"      : "load",
                "scaling_factor" : "(sin(((t)/0.02)/2500*pi/2))**2",
                //"scaling_factor" : "((sin(((t+7.383945)/0.001)/10000*pi/2))**2)-0.84",
                //"scaling_factor" : "((sin(((t+7.956)/0.001)/10000*pi/2))**2)-0.9",
                "echo_level"     : 4,
                "interval"       : [0.0, 50.0]
                //"interval"       : [0.0, 2.0491]
            },
            "scaling_loads_2" : {
                "type"           : "scaling",
                "solver"         : "structure",
                "data_name"      : "load",
                "scaling_factor" : "0.1",
                "echo_level"     : 4,
                "interval"       : [0, 1.0]
            },
            "scaling_loads_3" : {
                "type"           : "scaling",
                "solver"         : "structure",
                "data_name"      : "load",
                "scaling_factor" : "0.2",
                "echo_level"     : 4,
                "interval"       : [1.0, 2.0]
            },
            "scaling_loads_4" : {
                "type"           : "scaling",
                "solver"         : "structure",
                "data_name"      : "load",
                "scaling_factor" : "0.3",
                "echo_level"     : 4,
                "interval"       : [2.0, 3.0]
            },
            "scaling_loads_5" : {
                "type"           : "scaling",
                "solver"         : "structure",
                "data_name"      : "load",
                "scaling_factor" : "0.4",
                "echo_level"     : 4,
                "interval"       : [3.0, 20.0]
            },
            "scaling_loads_6" : {
                "type"           : "scaling",
                "solver"         : "structure",
                "data_name"      : "load",
                "scaling_factor" : "0.5",
                "echo_level"     : 4,
                "interval"       : [20.0, 50.0]
            },
            "scaling_loads_7" : {
                "type"           : "scaling",
                "solver"         : "structure",
                "data_name"      : "load",
                "scaling_factor" : "0.6",
                "echo_level"     : 4,
                "interval"       : [50.0, 100.0]
            },
            "mapping_element_node_1" : {
                "type"           : "elemental_data_to_nodal_data",
                "solver"         : "Openfoam_Kratos_Wrapper",
                "data_name"      : "load_cells",
                "echo_level"     : 4
                //"interval"       : [0.04, 100.0]
            },
            "coupling_output_1" : {
                "type"          : "coupling_output",
                "solver"        : "Openfoam_Kratos_Wrapper",
                "execution_point" : "finalize_solution_step",
                "output_parameters" :  {
                    "model_part_name"                             : "interface_flap",
                    "output_control_type"                         : "step",
                    "output_interval"                             : 1,
                    "file_format"                                 : "ascii",
                    "output_precision"                            : 7,
                    "output_sub_model_parts"                      : false,
                    "output_path"                                 : "vtk_output_coupling",
                    "save_output_files_in_folder"                 : true,
                    "nodal_solution_step_data_variables"          : ["FORCE"],
                    "element_data_value_variables"                : ["FORCE"]
                }
            }
        },
        "solvers" :
        {
            "Openfoam_Kratos_Wrapper":
            {
                "type" : "solver_wrappers.external.openfoam_wrapper",
                "solver_wrapper_settings" : {
                    "import_meshes"    : ["interface_flap"],
                    "export_data"      : ["disp"],
                    "import_data"      : ["load_cells"]
                },
                "io_settings" : {
                    "type"       : "kratos_co_sim_io",
                    "echo_level" : 4,
                    "connect_to" : "Openfoam_Adapter"
                },
                "data" : {
                    "disp" : {
                        "model_part_name"   : "interface_flap",
                        "dimension" : 3,
                        "variable_name" : "DISPLACEMENT"
                    },
                    "load_cells" : {
                        "model_part_name"   : "interface_flap",
                        "location" : "element",
                        "dimension" : 3,
                        "variable_name" : "FORCE"
                    },
                    "load" : { // same as load_cells but has data on nodes => required for the mapper!
                        "model_part_name"   : "interface_flap",
                        "dimension" : 3,
                        "variable_name" : "FORCE"
                    }
                }
            },
            "structure" :
            {
                "type" : "solver_wrappers.kratos.structural_mechanics_wrapper",
                "solver_wrapper_settings" : {
                    "input_file"  : "ProjectParametersCSM"
                },
                "data" : {
                    "disp" : {
                        "model_part_name" : "Structure.GENERIC_FSI_Interface",
                        "variable_name" : "DISPLACEMENT",
                        "dimension" : 3
                    },
                    "load" : {
                        "model_part_name" : "Structure.GENERIC_FSI_Interface",
                        "variable_name" : "POINT_LOAD",
                        "dimension" : 3
                    }
                }
            }
        }
    }
}
