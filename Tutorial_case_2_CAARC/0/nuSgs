FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuSgs;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }

    upperWall
    {
        type            zeroGradient;
    }

    outlet
    {
	type            zeroGradient;
    }

    lowerWall
    {
        type            zeroGradient;
    }

    tubesfs
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            symmetry;
    }



}

